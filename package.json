{
    "name": "smartopenfile",
    "displayName": "smartOpenFile",
    "description": "Open files smarter. Quick access to siblings.",
    "version": "0.0.1",
    "publisher": "nocksock",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:smartOpenFile.open"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "smartOpenFile.open",
                "title": "Smart Open File: Open File"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "q": "^1.5.0"
    }
}
